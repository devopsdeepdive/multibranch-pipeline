pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo 'Build is unsuccsessful'
            }
        }
        stage('Test') { 
            steps {
                echo 'Testing Complete' 
            }
        }
         stage('Package') { 
            steps {
                echo 'package crated successfully' 
            }
        }
        stage('Deploy') { 
            steps {
                echo 'Successfully deployed' 
            }
        }
        stage('Notify') { 
            steps {
                echo 'Notification Sent' 
            }
        }
    }
} 
